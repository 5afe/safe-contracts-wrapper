{
  "name": "@consideritdone/safe-proxy-factory-wrapper",
  "version": "1.0.0",
  "description": "Gnosis safe proxy factory wrapper",
  "files": [
    "contracts",
    "dist",
    "src",
    "test",
    "build"
  ],
  "scripts": {
    "build": "hardhat build-abi && yarn build:polywrap",
    "build:polywrap": "npx polywrap codegen && npx polywrap build -o build-wrapper",
    "test": "yarn test:e2e && yarn test:workflow",
    "test:e2e": "yarn test:e2e:codegen && jest --passWithNoTests --runInBand --verbose",
    "test:e2e:codegen": "npx polywrap app codegen -m ./wrapper/__tests__/types/polywrap.app.yaml -g ./wrapper/__tests__/types/wrap",
    "test:env:up": "npx polywrap infra up --modules=eth-ens-ipfs",
    "test:env:down": "npx polywrap infra down --modules=eth-ens-ipfs",
    "test:workflow": "yarn test:env:up && sleep 60 && npx polywrap run ./workflows/e2e.yaml -c ./workflows/config.ts -v ./workflows/validator.cue && yarn test:env:down"
  },
  "devDependencies": {
    "@gnosis.pm/mock-contract": "^4.0.0",
    "@openzeppelin/contracts": "^3.4.0",
    "@types/node": "^14.14.21",
    "@types/jest": "27.0.3",
    "argv": "^0.0.2",
    "dotenv": "^8.0.0",
    "hardhat": "^2.2.1",
    "jest": "26.6.3",
    "ts-jest": "26.5.4",
    "solc": "0.7.6",
    "polywrap": "0.4.1",
    "assemblyscript": "0.19.1",
    "@polywrap/wasm-as": "0.4.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
