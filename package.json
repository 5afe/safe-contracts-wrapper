{
  "name": "safe-wrapper",
  "description": "",
  "license": "MIT",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "./packages/safe-contracts-wrapper",
      "./packages/safe-factory-wrapper",
      "./packages/safe-wrapper"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/5afe/safe-contracts-wrapper"
  },
  "scripts": {
    "build": "yarn build:contracts && yarn build:factory && yarn build:safe",
    "build:contracts": "lerna run build --scope @consideritdone/safe-contracts-wrapper",
    "build:factory": "lerna run build --scope @consideritdone/safe-factory-wrapper",
    "build:safe": "lerna run build --scope @consideritdone/safe-wrapper",
    "test": "yarn test:contracts && yarn test:factory && yarn test:safe",
    "test:contracts": "lerna run test:e2e --concurrency 1 --scope @consideritdone/safe-contracts-wrapper",
    "test:factory": "lerna run test --concurrency 1 --scope @consideritdone/safe-factory-wrapper",
    "test:safe": "lerna run test --concurrency 1 --scope @consideritdone/safe-wrapper"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "lerna": "5.5.2",
    "polywrap": "~0.10.0-pre"
  }
}
