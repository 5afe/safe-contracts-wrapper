{
  "name": "@consideritdone/safe-wrapper",
  "version": "1.0.0",
  "description": "Gnosis safe wrappers",
  "files": [
    "contracts",
    "dist",
    "src",
    "test",
    "build"
  ],
  "scripts": {
    "build": "polywrap build",
    "codegen": "polywrap codegen",
    "test": "yarn test:e2e",
    "test:e2e": "yarn test:e2e:codegen && jest --passWithNoTests --runInBand --verbose",
    "test:e2e:codegen": "polywrap codegen -m ./src/__tests__/types/polywrap.app.yaml -g ./src/__tests__/types/wrap",
    "test:debug": "jest src/__tests__/e2e/typedData.integration.spec --passWithNoTests --runInBand --verbose",
    "test:createTransaction": "jest src/__tests__/e2e/createTransaction.spec --passWithNoTests --runInBand --verbose",
    "test:execution": "jest src/__tests__/e2e/execution.spec --passWithNoTests --runInBand --verbose",
    "test:onChainSignatures": "jest src/__tests__/e2e/onChainSignatures.spec --passWithNoTests --runInBand --verbose",
    "test:offChainSignatures": "jest src/__tests__/e2e/offChainSignatures.spec --passWithNoTests --runInBand --verbose",
    "test:integration:createTransaction": "jest src/__tests__/e2e/createTransaction.integration --passWithNoTests --runInBand --verbose",
    "test:integration:signTransaction": "jest src/__tests__/e2e/signTransaction.integration --passWithNoTests --runInBand --verbose"
  },
  "devDependencies": {
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@gnosis.pm/safe-core-sdk": "3.1.1",
    "@polywrap/core-js": "0.9.2",
    "@polywrap/wasm-as": "0.9.2",
    "@types/jest": "26.0.8",
    "assemblyscript": "0.19.1",
    "ethereumjs-util": "^7.1.5",
    "ganache": "^7.5.0",
    "jest": "26.6.3",
    "polywrap": "^0.9.3",
    "semver": "^7.3.8",
    "truffle": "^5.6.4",
    "ts-jest": "26.5.4"
  },
  "dependencies": {
    "@gnosis.pm/safe-ethers-lib": "^1.6.1",
    "ethers": "5.7.1",
    "polywrap-datetime-plugin": "npm:@fetsorn/polywrap-datetime-plugin@0.4.0"
  }
}
