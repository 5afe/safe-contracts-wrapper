{
  "name": "@consideritdone/safe-factory-wrapper",
  "version": "1.0.0",
  "description": "Gnosis safe proxy factory wrapper",
  "files": [
    "contracts",
    "dist",
    "src",
    "test",
    "build"
  ],
  "scripts": {
    "prebuild": "ts-node ./scripts/parseContractFiles",
    "build": "polywrap build",
    "codegen": "polywrap codegen",
    "test": "yarn test:e2e",
    "test:e2e": "yarn test:e2e:codegen && jest --passWithNoTests --runInBand --verbose",
    "test:e2e:codegen": "polywrap codegen -m ./src/__tests__/types/polywrap.app.yaml -g ./src/__tests__/types/wrap"
  },
  "devDependencies": {
    "@gnosis.pm/safe-contracts_1.2.0": "npm:@gnosis.pm/safe-contracts@1.2.0",
    "@gnosis.pm/safe-contracts_1.3.0": "npm:@gnosis.pm/safe-contracts@1.3.0",
    "@gnosis.pm/safe-deployments": "^1.16.0",
    "@polywrap/core-js": "0.7.0",
    "@polywrap/wasm-as": "0.7.0",
    "@types/jest": "26.0.8",
    "assemblyscript": "0.19.1",
    "jest": "26.6.3",
    "polywrap": "0.7.0",
    "ts-jest": "26.5.4"
  },
  "dependencies": {
    "polywrap-datetime-plugin": "^0.3.1"
  }
}
